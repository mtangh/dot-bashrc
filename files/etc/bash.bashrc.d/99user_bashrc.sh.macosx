# ${bashrc_dir}/99user_bashrc.sh.macosx
# $Id$

: "user_bashrc for macOS" && {

  # Load scripts under the 'bash_profile.d' dir
  for dot_prof_scr in $( {
  __pf_rc_loader \
  "${HOME}"/.{macOS,MacOSX}/{etc/,}{bash_,}profile.d
  } 2>/dev/null || :; )
  do
    [ -f "${dot_prof_scr}" ] &&
    [ -x "${dot_prof_scr}" ] &&
    . "${dot_prof_scr}" || :
  done
  unset dot_prof_scr

} 1>/dev/null 2>&1 || :

